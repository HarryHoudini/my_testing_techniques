                    <include name="moveFreeStoreyStructure" />
                </methods>
            </class>
            <class name="test.e2e.widgets.nettree.ActionsWithRootOfNetworkTree">
                <methods>
                    <include name="addNewGroupToGroup" />
                    <include name="addNewDeviceToGroupUsingContexMenu" />
                </methods>
            </class>
        </classes>
    </test>
</suite>
