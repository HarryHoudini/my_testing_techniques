                <methods>
                    <include name="addNewDeviceToRootUsingButtonShortForm" />
                    <include name="addNewDeviceToGroupUsingButtonShortForm" />
                    <include name="addNewDeviceToRootUsingButtonLongForm" />
                    <include name="simpleSearch" />
                    <include name="sortingByMainFieldNotIp" />
                    <include name="sortingByAdditionalFieldNotIp" />
                    <include name="sortingByMainFieldIp" />
                    <include name="sortingByAdditionalFieldIp" />
                    <include name="reverseSortingByMainField" />
                    <include name="reverseSortingByAdditionalField" />
                    <include name="selectHostnameMainField" />
                    <include name="selectIpMainField" />
                    <include name="selectMacMainField" />
                    <include name="selectCaptionMainField" />
                    <include name="selectIpAdditionalField" />
                    <include name="selectMacAdditionalField" />
                    <include name="selectCaptionAdditionalField" />
                    <include name="selectNoneAdditionalField" />
                    <include name="selectIpMainSameAdditionalField" />
                    <include name="selectMacMainSameAdditionalField" />
                    <include name="selectIpAdditionalSameMainField" />
                    <include name="selectMacAdditionalSameMainField" />
                    <include name="selectCaptionAdditionalSameMainField" />
